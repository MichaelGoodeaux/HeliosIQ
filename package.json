        "dev": "npm run dev --workspaces --parallel",
        "format": "prettier --write \"**/*.{ts,tsx,md}\""
    },
    "devDependencies": {
        "@types/node": "^22.10.2",
        "@typescript-eslint/eslint-plugin": "^6.7.0",
