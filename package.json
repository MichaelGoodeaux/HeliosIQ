        "eslint": "^8.49.0",
        "eslint-config-prettier": "^9.0.0",
        "prettier": "^3.0.3",
        "tsconfig-paths": "^4.2.0",
        "typescript": "^5.7.2"
    },
    "dependencies": {
        "@heliosiq/shared": "file:./package/shared",
        "axios": "^1.7.9",
        "prom-client": "^15.1.3",
        "zod": "^3.24.1"
    }
